{
  "name": "@seamless-scroll/vue",
  "version": "0.2.0",
  "description": "Vue3 component for seamless scrolling",
  "keywords": [
    "scroll",
    "seamless",
    "vue3"
  ],
  "author": "yeyunwen",
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeyunwen/seamless-scroll.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/yeyunwen/seamless-scroll/issues"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "dependencies": {
    "@seamless-scroll/core": "workspace:*",
    "@seamless-scroll/shared": "workspace:*"
  },
  "devDependencies": {
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  }
}
